{
  "name": "@defi-dao-fund/indexer",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "db:migrate": "npx squid-typeorm-migration apply",
    "db:create": "npx squid-typeorm-migration create",
    "codegen": "npx squid-typeorm-codegen",
    "typegen": "squid-evm-typegen --abi ./src/abi",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "start": "npm run build && npm run db:migrate && npm run processor:start",
    "test": "jest",
    "lint": "eslint . --ext .ts --ignore-path .gitignore"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/evm-processor": "^1.13.0",
    "@subsquid/graphql-server": "^4.5.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.2.6",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^3.2.3",
    "@subsquid/typeorm-codegen": "^1.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3",
    "ts-jest": "^29.1.2"
  }
}